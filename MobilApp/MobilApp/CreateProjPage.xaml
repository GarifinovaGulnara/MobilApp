             x:Class="MobilApp.CreateProjPage"
             Title="Создать проект">
    <ContentPage.Content>
        <StackLayout>
            <Frame>
                <Entry Text="Имя проекта"/>
            </Frame>
            <Frame>
                <Entry Text="Описание"/>
            </Frame>
            <Frame>
                <Entry Text="Телефон №1"/>
            </Frame>
            <Frame>
                <Entry Text="E-mail"/>
            </Frame>
            <Frame>
                <Entry Text="Адрес"/>
            </Frame>
            <Grid>
                <Button Text="Отмена" Margin="55, 20,200,0"/>
